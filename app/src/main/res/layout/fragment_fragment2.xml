<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ramosjanoah.simplicity.Fragment2">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
        android:text="Muscle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:id="@+id/MuscleTextView"
            android:layout_below="@+id/HealthTextView"
            android:layout_centerHorizontal="true" />

        <TextView
            android:text="Health"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="34dp"
            android:id="@+id/HealthTextView"
            android:layout_alignParentTop="true"
            android:layout_alignLeft="@+id/MuscleTextView"
            android:layout_alignStart="@+id/MuscleTextView" />
    </RelativeLayout>
</FrameLayout>
